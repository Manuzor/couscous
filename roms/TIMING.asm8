LD Va 1C  # x
LD Vb 0C  # y

# DT frequency
LD V9 1E

:LOOP
LD V0 DT
SE V0 0
CALL LOAD_INNER
SNE V0 0
CALL LOAD_OUTER
DRW Va Vb 8
XOR Vf Vf  # pad to a 10-instruction-loop
JP LOOP

:LOAD_INNER
LD I SPRITE_INNER
XOR Vf Vf  # no-op/pad
RET

:LOAD_OUTER
LD I SPRITE_OUTER
LD DT V9
RET

:SPRITE_INNER
0b00000000
0b00000000
0b00111100
0b00111100
0b00111100
0b00111100
0b00000000
0b00000000

:SPRITE_OUTER
0b11111111
0b10000001
0b10000001
0b10000001
0b10000001
0b10000001
0b10000001
0b11111111
